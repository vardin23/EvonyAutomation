#
#  Menu Entry 0       Windows EFI Boot Manager
#
#  ** Grub will boot this entry by default **
#
menuentry   'Windows EFI Boot Manager                          Hotkey=w'   --hotkey=w    --class windows   --class icon-windows  {
     set reviewpause=1
     set efibootmgr=/efi/Microsoft/Boot/bootmgfw.efi
     search -u $grub2win_efiuuid --set root
     if [ $? = 0 ] -a  [ -f ($root)$efibootmgr ]   ; then 
        echo ; echo The Windows EFI partition UUID $grub2win_efiuuid was found on $root ; echo ;
        else getpartition  file  $efibootmgr  root ; fi
     if [ ! -z $reviewpause ] ; then
	 echo GNU Grub will load the Windows EFI Boot Manager at disk address $root
         g2wsleep  
     fi
     echo GNU Grub is now loading the Windows EFI Boot Manager
     chainloader $efibootmgr
     savelast 0 'Windows EFI Boot Manager'
}


#
#  Menu Entry 1       Manjaro Linux
#
menuentry   'Manjaro Linux                                     Hotkey=m'   --hotkey=m    --class manjaro   --class icon-manjaro  {
     set reviewpause=2
     echo GNU Grub is preparing to boot  Manjaro Linux
     set gfxpayload=1024x768
     set rootuuid=EAA931EB-4301-44B7-A996-A413F5A1FE63
     getpartition  uuid  $rootuuid  root
     g2wsetprefix
     echo Boot disk address is  $root
     echo The boot mode is      Partition UUID
     g2wsleep
     linux   $pathprefix/vmlinuz   root=UUID=$rootuuid    rw verbose
     if [ -f $pathprefix/amd-ucode.img ]   ; then set ucode=/amd-ucode.img   ; fi
     if [ -f $pathprefix/intel-ucode.img ] ; then set ucode=/intel-ucode.img ; fi
     initrd  $pathprefix/$ucode  $pathprefix/initramfs.img
     if [ $? -ne 0 ] ; then g2werror Linux load error ; fi
     savelast 1 'Manjaro Linux'
}


